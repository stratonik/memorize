<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HorizontalPanel width="100%">
		<g:VerticalPanel horizontalAlignment="center">
			<g:Button ui:field="stop" text="Прервать" addStyleNames="trainingButton" />
			<g:Label ui:field="counter" addStyleNames="trainingCounter" />
		</g:VerticalPanel>
		<g:cell width="100%" horizontalAlignment="center">
			<g:SimplePanel ui:field="testWidget" styleName="trainingWidget" />
		</g:cell>
		<g:VerticalPanel horizontalAlignment="center">
			<g:Button ui:field="next" text="Дальше >" addStyleNames="trainingButton" />
			<g:Label ui:field="timer" addStyleNames="trainingTimer" />
		</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder>