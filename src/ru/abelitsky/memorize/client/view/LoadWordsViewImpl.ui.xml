<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:VerticalPanel width="100%" spacing="5">
		<g:Label text="Загрузка слов" styleName="title" />
		<g:Label
			text="Вставте сюда содержимое csv-файла (порядок полей: слово, иероглифы, перевод, дополнительная информация):" />
		<g:TextArea ui:field="data" width="100%" visibleLines="25" />
		<g:HorizontalPanel styleName="actions">
			<g:Button ui:field="load" text="Загрузить" />
			<g:Button ui:field="cancel" text="Отмена" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>