<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:Anchor ui:field="currentCourses" text="&lt;&lt; Назад к выбранным курсам"
			styleName="navigation" />
		<g:Label text="Все курсы:" styleName="title" />
		<g:FlexTable ui:field="list" width="100%" />
		<div class="actions">
			<g:Button ui:field="newCourse" text="Новый курс..." />
		</div>
	</g:HTMLPanel>
</ui:UiBinder>